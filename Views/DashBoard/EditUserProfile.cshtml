@using RunGroopApp.ViewModels
@model EditUserDashBoardViewModel
@{
    ViewData["Title"] = "Editing";
}
<form method="post" asp-action="Create" enctype="multipart/form-data">
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label asp-for="Pace"></label>
        <input asp-for="Pace" type="text" class="form-control" placeholder="Enter Pace">
        <span asp-validation-for="Pace" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Mileage"></label>
        <input asp-for="Mileage" type="text" class="form-control" placeholder="Enter Mileage">
        <span asp-validation-for="Mileage" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="City"></label>
        <input asp-for="City" type="text" class="form-control" placeholder="Enter City">
        <span asp-validation-for="City" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="State"></label>
        <input asp-for="State" type="text" class="form-control" placeholder="Enter State">
        <span asp-validation-for="State" class="text-danger"></span>

    </div>

    <div class="form-group">
        <label asp-for="Image">Profile image</label>
        <input asp-for="Image" type="file" class="form-control" placeholder="Enter image">
        <span asp-validation-for="Image" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>

</form>